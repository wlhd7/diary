# Example environment variables for local development.
# Copy this to `.env` or export the variables in your shell.

# MySQL connection
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=diary

# Flask
FLASK_APP=diary:create_app
FLASK_ENV=development
 
# Application secret (use a strong random value in production)
SECRET_KEY=replace-with-a-strong-secret
